SET foreign_key_checks = 1;

-- ==================== Ejemplo de uso de stored procedures ====================

-- Ejemplo de uso: registrar un pago
CALL registrar_pago(1, 500.00, 'Tarjeta', 'REF1001');

-- Ejemplo de uso: crear un plan de pago
CALL crear_plan_pago(2, 83.58, 'Mensual', 5);


-- Ejemplo de uso: generar reporte diario de morosidad
CALL generar_reporte_morosidad();

-- ==================== Ejemplo de uso de funciones ====================

-- Ejemplo: calcular días de mora para la deuda 1
SELECT calcular_dias_mora(1) AS dias_mora;

-- Ejemplo: calcular saldo con intereses para la deuda 1
SELECT calcular_saldo_interes(1) AS saldo_con_interes;

-- ==================== Ejemplo en vistas (consultas de ejemplo) ====================

-- Ejemplo: consultar la vista de cartera vencida
SELECT * FROM vw_cartera_vencida WHERE id_deuda = 1;

-- Ejemplo: consultar la vista de efectividad de agentes
SELECT * FROM vw_efectividad_agentes WHERE id_agente = 1;

-- Ejemplo: consultar el historial de pagos de un cliente
SELECT * FROM vw_historial_pagos_cliente WHERE id_cliente = 1;

-- Ejemplo: consultar los planes de pago y su estado
SELECT * FROM vw_estado_planes_pago WHERE id_deuda = 1;

-- Ejemplo: consultar alertas pendientes
SELECT * FROM vw_alertas_pendientes WHERE id_alerta = 1;

